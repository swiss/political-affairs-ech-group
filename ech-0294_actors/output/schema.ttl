    skos:inScheme <https://ch.paf.link/schema/actors> .

act:AddressTypeEnum a owl:Class ;
    rdfs:label "AddressTypeEnum" ;
    owl:unionOf ( <https://ch.paf.link/schema/actors/AddressTypeEnum#privateAddress> <https://ch.paf.link/schema/actors/AddressTypeEnum#businessAddress> <https://ch.paf.link/schema/actors/AddressTypeEnum#localAddress> ) ;
    skos:inScheme <https://ch.paf.link/schema/actors> ;
    linkml:permissible_values <https://ch.paf.link/schema/actors/AddressTypeEnum#businessAddress>,
        <https://ch.paf.link/schema/actors/AddressTypeEnum#localAddress>,
        <https://ch.paf.link/schema/actors/AddressTypeEnum#privateAddress> .

act:Validity a owl:Class ;
    rdfs:label "Validity" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty act:valid_until ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:date ;
            owl:onProperty act:valid_until ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:date ;
            owl:onProperty act:valid_from ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty act:valid_from ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty act:valid_until ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty act:valid_from ] ;
    skos:inScheme <https://ch.paf.link/schema/actors> .

