    skos:inScheme <https://ch.paf.link/schema/operations> .

ops:state a owl:DatatypeProperty ;
    rdfs:label "state" ;
    skos:inScheme <https://ch.paf.link/schema/operations> .

ops:type a owl:DatatypeProperty ;
    rdfs:label "type" ;
    skos:inScheme <https://ch.paf.link/schema/operations> .

ops:updated_datetime a owl:DatatypeProperty ;
