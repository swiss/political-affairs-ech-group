@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix tutorial: <https://ch.paf.link/schema/tutorial/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://ch.paf.link/schema/td/date_actual> a owl:DatatypeProperty ;
    rdfs:label "date_actual" ;
    rdfs:range xsd:date ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/td/date_begin_planned> a owl:DatatypeProperty ;
    rdfs:label "date_begin_planned" ;
    rdfs:range xsd:date ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/td/date_created> a owl:DatatypeProperty ;
    rdfs:label "date_created" ;
    rdfs:range xsd:date ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/td/date_end_planned> a owl:DatatypeProperty ;
    rdfs:label "date_end_planned" ;
    rdfs:range xsd:date ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/td/date_modified> a owl:DatatypeProperty ;
    rdfs:label "date_modified" ;
    rdfs:range xsd:date ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/td/date_planned> a owl:DatatypeProperty ;
    rdfs:label "date_planned" ;
    rdfs:range xsd:date ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/td/datetime_begin_actual> a owl:DatatypeProperty ;
    rdfs:label "datetime_begin_actual" ;
    rdfs:range xsd:dateTime ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/td/datetime_begin_planned> a owl:DatatypeProperty ;
    rdfs:label "datetime_begin_planned" ;
    rdfs:range xsd:dateTime ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/td/datetime_created> a owl:DatatypeProperty ;
    rdfs:label "datetime_created" ;
    rdfs:range xsd:dateTime ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/td/datetime_end_actual> a owl:DatatypeProperty ;
    rdfs:label "datetime_end_actual" ;
    rdfs:range xsd:dateTime ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/td/datetime_end_planned> a owl:DatatypeProperty ;
    rdfs:label "datetime_end_planned" ;
    rdfs:range xsd:dateTime ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/td/datetime_modified> a owl:DatatypeProperty ;
    rdfs:label "datetime_modified" ;
    rdfs:range xsd:dateTime ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/td/datetime_planned> a owl:DatatypeProperty ;
    rdfs:label "datetime_planned" ;
    rdfs:range xsd:dateTime ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/tutorial.owl.ttl> a owl:Ontology ;
    rdfs:label "tutorial" ;
    skos:definition """[en] This is a dummy schema for a parliamentary affairs system, including sessions, agenda items, and votes. It is used to demonstrate the use of LinkML for defining data models. In this model, a session contains multiple agenda items and each agenda item can have multiple votes.
[de] Dies ist ein Dummy-Schema für ein System von parlamentarischen Angelegenheiten, das Sitzungen, Tagesordnungspunkte und Abstimmungen umfasst. Es wird verwendet, um die Verwendung von LinkML zur Definition von Datenmodellen zu demonstrieren. In diesem Modell enthält eine Sitzung mehrere Tagesordnungspunkte und jeder Tagesordnungspunkt kann mehrere Abstimmungen haben.
""" .

tutorial:Container a owl:Class ;
    rdfs:label "Container" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty tutorial:votes ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty tutorial:agenda_items ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty tutorial:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom tutorial:Session ;
            owl:onProperty tutorial:sessions ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty tutorial:sessions ],
        [ a owl:Restriction ;
            owl:allValuesFrom tutorial:Vote ;
            owl:onProperty tutorial:votes ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty tutorial:id ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty tutorial:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom tutorial:AgendaItem ;
            owl:onProperty tutorial:agenda_items ] ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

tutorial:Session a owl:Class ;
    rdfs:label "Session" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://ch.paf.link/schema/td/date_begin_actual> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://ch.paf.link/schema/td/date_end_actual> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://ch.paf.link/schema/td/date_begin_actual> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty tutorial:agenda_items ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty tutorial:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:date ;
            owl:onProperty <https://ch.paf.link/schema/td/date_end_actual> ],
        [ a owl:Restriction ;
            owl:allValuesFrom tutorial:AgendaItem ;
            owl:onProperty tutorial:agenda_items ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://ch.paf.link/schema/td/date_end_actual> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:date ;
            owl:onProperty <https://ch.paf.link/schema/td/date_begin_actual> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty tutorial:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty tutorial:name ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty tutorial:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom tutorial:MultilingualString ;
            owl:onProperty tutorial:name ] ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

<https://ch.paf.link/schema/tutorial/results/no> a owl:Class ;
    rdfs:label "no" ;
    rdfs:subClassOf tutorial:ResultEnum .

<https://ch.paf.link/schema/tutorial/results/yes> a owl:Class ;
    rdfs:label "yes" ;
    rdfs:subClassOf tutorial:ResultEnum .

tutorial:sessions a owl:ObjectProperty ;
    rdfs:label "sessions" ;
    rdfs:range tutorial:Session ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

<https://ch.paf.link/schema/td/date_begin_actual> a owl:DatatypeProperty ;
    rdfs:label "date_begin_actual" ;
    rdfs:range xsd:date ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/td/date_end_actual> a owl:DatatypeProperty ;
    rdfs:label "date_end_actual" ;
    rdfs:range xsd:date ;
    skos:inScheme <https://ch.paf.link/schema/td> .

<https://ch.paf.link/schema/td/datetime_actual> a owl:DatatypeProperty ;
    rdfs:label "datetime_actual" ;
    rdfs:range xsd:dateTime ;
    skos:inScheme <https://ch.paf.link/schema/td> .

tutorial:MultilingualString a owl:Class ;
    rdfs:label "MultilingualString" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:onDatatype xsd:string ;
                    owl:withRestrictions ( [ xsd:pattern "^[a-z]{2}$" ] ) ] ;
            owl:onProperty tutorial:language ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty tutorial:text ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty tutorial:text ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty tutorial:language ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty tutorial:text ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty tutorial:language ] ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

tutorial:Vote a owl:Class ;
    rdfs:label "Vote" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty <https://ch.paf.link/schema/td/datetime_actual> ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty tutorial:is_part_of ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty tutorial:result ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty tutorial:result ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:anyURI ;
            owl:onProperty tutorial:is_part_of ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty tutorial:question ],
        [ a owl:Restriction ;
            owl:allValuesFrom tutorial:ResultEnum ;
            owl:onProperty tutorial:result ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty tutorial:question ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty tutorial:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty <https://ch.paf.link/schema/td/datetime_actual> ],
        [ a owl:Restriction ;
            owl:allValuesFrom tutorial:AgendaItem ;
            owl:onProperty tutorial:is_part_of_agenda_item ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty tutorial:is_part_of_agenda_item ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty <https://ch.paf.link/schema/td/datetime_actual> ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty tutorial:is_part_of_agenda_item ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty tutorial:is_part_of ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty tutorial:id ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty tutorial:question ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty tutorial:id ] ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

tutorial:is_part_of a owl:DatatypeProperty ;
    rdfs:label "is_part_of" ;
    rdfs:range xsd:anyURI ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

tutorial:is_part_of_agenda_item a owl:ObjectProperty ;
    rdfs:label "is_part_of_agenda_item" ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

tutorial:language a owl:DatatypeProperty ;
    rdfs:label "language" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:onDatatype xsd:string ;
            owl:withRestrictions ( [ xsd:pattern "^[a-z]{2}$" ] ) ] ;
    skos:definition """[en] Language code in ISO 639-1 format.
[de] Sprachcode im ISO 639-1-Format.
""" ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

tutorial:question a owl:DatatypeProperty ;
    rdfs:label "question" ;
    rdfs:range xsd:string ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

tutorial:result a owl:ObjectProperty ;
    rdfs:label "result" ;
    rdfs:range tutorial:ResultEnum ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

tutorial:text a owl:DatatypeProperty ;
    rdfs:label "text" ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

tutorial:AgendaItem a owl:Class ;
    rdfs:label "AgendaItem" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom tutorial:Vote ;
            owl:onProperty tutorial:votes ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty tutorial:id ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty tutorial:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty tutorial:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty tutorial:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty tutorial:votes ],
        [ a owl:Restriction ;
            owl:allValuesFrom tutorial:MultilingualString ;
            owl:onProperty tutorial:name ] ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

tutorial:ResultEnum a owl:Class ;
    rdfs:label "result_enum" ;
    owl:unionOf ( <https://ch.paf.link/schema/tutorial/results/yes> <https://ch.paf.link/schema/tutorial/results/no> ) ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> ;
    linkml:permissible_values <https://ch.paf.link/schema/tutorial/results/no>,
        <https://ch.paf.link/schema/tutorial/results/yes> .

tutorial:agenda_items a owl:ObjectProperty ;
    rdfs:label "agenda_items" ;
    rdfs:range tutorial:AgendaItem ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

tutorial:name a owl:ObjectProperty ;
    rdfs:label "name" ;
    rdfs:range tutorial:MultilingualString ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

tutorial:votes a owl:ObjectProperty ;
    rdfs:label "votes" ;
    rdfs:range tutorial:Vote ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .

tutorial:id a owl:DatatypeProperty ;
    rdfs:label "id" ;
    skos:inScheme <https://ch.paf.link/schema/tutorial> .


